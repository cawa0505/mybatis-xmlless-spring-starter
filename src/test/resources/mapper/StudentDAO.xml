<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aegis.mybatis.dao.StudentDAO">
  <resultMap id="student" type="com.aegis.mybatis.bean.Student">
    <id column="id" property="id"/>
    <collection property="scores"
                columnPrefix="score_"
                ofType="com.aegis.mybatis.bean.Score">
    </collection>
  </resultMap>
  <resultMap id="score" type="com.aegis.mybatis.bean.Score">

  </resultMap>
</mapper>
